              <Swiper 
                slidesPerView={6}
                slidesPerGroup={3}
                modules={[Navigation, Pagination, Autoplay]}
                className="max-w-[900px] duration-700 rounded-3xl"
                navigation
                pagination={{ clickable: true }}
                loop={true}
                speed={1200}
              >
                {aircomingTVs.map((series: any) => (
                  <SwiperSlide>
                    <ItemCard type="tv" item={series} />
                  </SwiperSlide>
                ))}
              </Swiper> 
